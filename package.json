    "build": "yarn run lint && node --experimental-json-modules esbuild.config.mjs production",
    "release": "standard-version"
  },
  "standard-version": {
    "t": ""
  },
  "dependencies": {
    "@popperjs/core": "2.11.5",
    "lodash": "4.17.21",
    "obsidian": "0.14.4",
